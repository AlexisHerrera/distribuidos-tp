[MAIN]
jobs=0

[MESSAGES CONTROL]
disable=
    W0703, ; broad-except
    C0114, ; missing-module-docstring
    C0115, ; missing-class-docstring
    C0116, ; missing-function-docstring
    R0903, ; too-few-public-methods
    C0103  ; invalid-name


[FORMAT]
max-line-length=120

[DESIGN]
max-args=7

max-attributes=15

max-locals=20

# [VARIABLES]
ignored-argument-names=_.*
dummy-variables-rgx=^_|dummy